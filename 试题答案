第一题 
# include <iostream>
# include <cstring>
using namespace std;
int main(){
	string s;
	getline(cin,s);
	int len;
	len=s.length();
	for(int i=0;i<len;i++){
		int ok=1;
		for(int j=0;j<len;j++){
			if(j!=i&&s[j]==s[i])
			ok=0;
		}
		if(ok==1){
			cout<<i;
			return 0;
		}
	}
	cout<<-1;
	return 0;
} 
第三题 
# include <iostream>
# include <algorithm>
using namespace std;
int main(){
	int a[100];
	char c[100];
	int ok[100];
	string b;
	getline(cin,b);
	int j=0;
	for(int i=0;b[i]!='\0';i++){
		if(b[i]=='['||b[i]==','){
			if(b[i+1]<'0'||b[i+1]>'9'){
				c[j]=b[i+2];
				ok[j]=0;
			    j++;
			}	
	
			else {int x=0;
			for(int m=i+1;b[m]!=','&&b[m]!=']';m++)
			x=(x+(b[m]-'0'))*10;
				a[j]=x/10;
				ok[j]=1;
				j++;
			}
		}	
	}
	for(int x=0;x<j;x++){
		if(ok[x]==0)
		a[x]=c[x]-'0';
	} 
	for(int m=0;m<j+1;m++){
		for(int n=0;n<j-m+1;n++){
			if(a[n]>a[n+1]){
				int t;
				t=a[n+1];
				a[n+1]=a[n];
				a[n]=t;
				char k;
				k=c[n+1];
				c[n+1]=c[n];
				c[n]=k;
				int s;
				s=ok[n+1];
				ok[n+1]=ok[n];
				ok[n]=s;
			}
		}
	}
	cout<<"[";
	for(int x=1;x<j;x++){
	if(ok[x]==0)
		cout<<"'"<<c[x]<<"',";
		else
		cout<<a[x]<<",";
	} 
	if(ok[j]==0)
		cout<<"'"<<c[j]<<"']";
		else
		cout<<a[j]<<"]";
	return 0;
}
